<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>transferBean</managed-bean-name>
		<managed-bean-class>ch.hevs.managedbeans.TransferBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>welcomeBank.xhtml</display-name>
		<from-view-id>/welcomeBank.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>subjectList</from-outcome>
			<to-view-id>/subjectList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcomeBank.xhtml</display-name>
		<from-view-id>/welcomeBank.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>classList</from-outcome>
			<to-view-id>/classList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>subjectList.xhtml</display-name>
		<from-view-id>/subjectList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>subjectUpdate</from-outcome>
			<to-view-id>/subjectUpdate.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>subjectList.xhtml</display-name>
		<from-view-id>/subjectList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createSubject</from-outcome>
			<to-view-id>/createSubject.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>studentList.xhtml</display-name>
		<from-view-id>/studentList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createStudent</from-outcome>
			<to-view-id>/createStudent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>studentList.xhtml</display-name>
		<from-view-id>/studentList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addMarkToStudent</from-outcome>
			<to-view-id>/addMarkToStudent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcomeBank.xhtml</display-name>
		<from-view-id>/welcomeBank.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>studentList</from-outcome>
			<to-view-id>/studentList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>classList.xhtml</display-name>
		<from-view-id>/classList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>transferStudent</from-outcome>
			<to-view-id>/transferStudent.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>transferStudent.xhtml</display-name>
		<from-view-id>/transferStudent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/errorpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>subjectUpdate.xhtml</display-name>
		<from-view-id>/subjectUpdate.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/errorpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>createSubject.xhtml</display-name>
		<from-view-id>/createSubject.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/errorpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>createStudent.xhtml</display-name>
		<from-view-id>/createStudent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/errorpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addMarkToStudent.xhtml</display-name>
		<from-view-id>/addMarkToStudent.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/errorpage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
